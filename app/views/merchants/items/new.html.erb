 <%= form_for [@merchant, @item] do |f| %>
  <%= f.label(:name)%>
  <%= f.text_field(:name )%>
  <%= f.label( :description)%>
  <%= f.text_field( :description) %>
  <%= f.label( :image_url)%>
  <%= f.text_field( :image_url) %>
  <%= f.label( :quantity)%>
  <%= f.number_field(:quantity) %>
  <%= f.label(:price)%>
  <%= f.number_field(:price) %>
  <%= f.submit("Add Item") %>
<% end  %>