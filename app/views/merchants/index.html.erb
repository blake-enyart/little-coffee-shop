<h1>Merchants</h1>

<section class = "merchants-container">
  <% @merchants.each do |merchant| %>
    <section id='merchant-<%= merchant.id %>'>
      <ul>
        <li><%= link_to merchant.name, merchant_path(merchant) %></li>
        <li><%= merchant.city%></li>
        <li><%= merchant.state%></li>
        <li><%= merchant.created_at%></li>
      </ul>
    </section>
  <% end %>
</section>
